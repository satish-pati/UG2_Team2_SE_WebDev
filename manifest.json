{
    "manifest_version": 3,
    "name": "Face Recognition Login",
    "version": "1.0",
    "description": "A Chrome extension that enables face recognition login.",
    "permissions": ["storage", "tabs", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
    
  }
  